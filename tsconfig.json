{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "strictPropertyInitialization": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "outDir": "build",
    "rootDir": ".",
    "lib": [
      "dom"
    ],
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts",
  ],
  "exclude": [
    "test/resources/**/*",
  ]
}