{
  "name": "website2pdf",
  "version": "0.0.0",
  "description": "Tool to export a website in PDFs based on its sitemap",
  "engines": {
    "node": ">=14.14.0"
  },
  "bin": {
    "website2pdf": "build/src/website2pdf.js"
  },
  "scripts": {
    "website2pdf": "ts-node src/website2pdf.ts",
    "test": "c8 --check-coverage mocha --recursive build/test/ --extension js",
    "mocha": "mocha --require ts-node/register --recursive test --extension ts",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "files": [
    "build/src",
    "CHANGELOG.md",
    "tsconfig.json",
    ".eslintrc.json",
    ".prettierrc.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jgazeau/website2pdf.git"
  },
  "keywords": [
    "crawler",
    "PDF",
    "generator"
  ],
  "author": "Jordan GAZEAU",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jgazeau/website2pdf/issues"
  },
  "homepage": "https://github.com/jgazeau/website2pdf#readme",
  "dependencies": {
    "axios": "^0.24.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "fast-xml-parser": "^3.21.1",
    "fs-extra": "^10.0.0",
    "kleur": "^4.1.4",
    "puppeteer": "^11.0.0",
    "reflect-metadata": "^0.1.13",
    "table": "^6.7.3",
    "tslog": "^3.2.2",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/chai-as-promised": "^7.1.4",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.10",
    "@types/sinon": "^10.0.6",
    "@types/sinon-chai": "^3.2.6",
    "@types/validator": "^13.7.0",
    "@types/yargs": "^17.0.7",
    "c8": "^7.10.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "github-release-notes": "^0.17.3",
    "gts": "^3.1.0",
    "mocha": "^9.1.3",
    "sinon": "^12.0.1",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
}
